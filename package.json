{
  "name": "plonk-ui",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf -- dist",
    "start": "npm run start:dev",
    "start:dev": "babel-node tools/srcServer.js",
    "start:prod": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "build:prod": "cross-env NODE_ENV=production babel-node node_modules/webpack/bin/webpack.js --config webpack.config.prod.js --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "test": "cross-env NODE_ENV=test mocha --require babel-register src/**/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha src/**/*.spec.js"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "3.10.1",
    "material-ui": "^0.16.6",
    "moment": "^2.17.1",
    "nes": "^6.4.0",
    "normalizr": "2.3.1",
    "object-assign": "4.1.0",
    "react": "15.4.1",
    "react-cookie": "^1.0.4",
    "react-dom": "15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "react-scrollbar": "^0.5.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "redux-form": "^6.4.2",
    "redux-form-material-ui": "^4.1.2",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "6.18.0",
    "browser-sync": "2.18.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "istanbul": "0.4.5",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mockdate": "2.0.1",
    "node-sass": "4.0.0",
    "npm-run-all": "3.1.2",
    "nyc": "^10.0.0",
    "open": "0.0.5",
    "postcss-loader": "1.2.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "dist"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
